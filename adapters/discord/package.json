    "directory": "adapters/discord"
  },
  "bugs": {
    "url": "https://github.com/satorijs/satori/issues"
  },
  "homepage": "https://koishi.chat/plugins/adapter/discord.html",
  "keywords": [
    "bot",
    "discord",
    "chatbot",
    "satori",
    "im",
    "chat"
  ],
  "devDependencies": {
    "@satorijs/core": "^4.5.0",
