{
  "name": "qqchannel-bot-admin",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev:client": "vite",
    "build:client": "vue-tsc --noEmit && vite build",
    "preview:client": "vite preview",
    "dev:server": "npx nodemon --watch server server/index.ts",
    "build:server": "ncc build ./server/index.ts -o dist/server",
    "build": "npm run build:client && npm run build:server",
    "preview": "node dist/bundle.js",
    "start": "pm2 start dist/bundle.js --name qqchannel-bot",
    "stop": "pm2 stop qqchannel-bot"
  },
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.1.0",
    "@heroicons/vue": "^2.0.11",
    "daisyui": "^2.27.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "lru-cache": "^7.14.0",
    "mitt": "^3.0.0",
    "mobx": "^6.6.2",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.22",
    "qq-guild-bot": "^2.9.5",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.37",
    "ws": "^8.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/node": "^18.7.17",
    "@types/sortablejs": "^1.13.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vercel/ncc": "^0.34.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.10",
    "cssnano": "^5.1.13",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "^9.4.0",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.40.4"
  }
}
